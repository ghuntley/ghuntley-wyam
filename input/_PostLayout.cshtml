@{
    Layout="_Layout";
    var Published = Model.Get<DateTime>(BlogKeys.Published);
    var Tags = Model.Get<string[]>(BlogKeys.Tags) ?? new string[] {};
}

<div class="article-open container no-cover">
  <div class="hover">
  </div>

  <article class="article-open_item wrap">
    <header class="post_head">
      <h1>@Model.WithoutSettings.String(BlogKeys.Title)</h1>


      <div class="post_socialIcons">
        <ul>
          <li class="socialIcons_link">
            <a class="fa fa-twitter" href="https://twitter.com/share?url=https://www.troyhunt.com/introducing-306-million-freely-downloadable-pwned-passwords/&amp;text=Troy%20Hunt%3A%20Introducing%20306%20Million%20Freely%20Downloadable%20Pwned%20Passwords" onclick="window.open(this.href, 'twitter-share', 'width=550,height=235');return false;"></a>
          </li>


          <li class="socialIcons_link">
            <a class="fa fa-facebook" href="https://www.facebook.com/sharer/sharer.php?u=https://www.troyhunt.com/introducing-306-million-freely-downloadable-pwned-passwords/" onclick="window.open(this.href, 'facebook-share','width=580,height=296');return false;"></a>
          </li>


          <li class="socialIcons_link">
            <a class="fa fa-google-plus" href="https://plus.google.com/share?url=https://www.troyhunt.com/introducing-306-million-freely-downloadable-pwned-passwords/" onclick="window.open(this.href, 'facebook-share','width=580,height=296');return false;"></a>
          </li>


          <li class="socialIcons_link linkedin">
            <a class="fa fa-linkedin" href="https://www.linkedin.com/shareArticle?mini=true&amp;url=https://www.troyhunt.com/introducing-306-million-freely-downloadable-pwned-passwords/" onclick="window.open(this.href, 'facebook-share','width=580,height=296');return false;"></a>
          </li>


          <li class="socialIcons_link reddit">
            <a class="fa fa-reddit-alien" href="https://reddit.com/submit?url=https://www.troyhunt.com/introducing-306-million-freely-downloadable-pwned-passwords/&amp;title=Troy%20Hunt%3A%20Introducing%20306%20Million%20Freely%20Downloadable%20Pwned%20Passwords" onclick="window.open(this.href, 'facebook-share','width=580,height=550');return false;"></a>
          </li>


          <li class="socialIcons_link">
            <a class="fa fa-envelope" href="https://mail.google.com/mail/?view=cm&amp;fs=1&amp;tf=1&amp;to=&amp;su=Troy%20Hunt%3A%20Introducing%20306%20Million%20Freely%20Downloadable%20Pwned%20Passwords&amp;body=https://www.troyhunt.com/introducing-306-million-freely-downloadable-pwned-passwords/" target="_blank"></a>
          </li>
        </ul>
      </div>
      <span class="post_info">@Published.ToLongDateString()</span>
    </header>
  </article>
</div>


<main class="article-open_content container">
  <article class="wrap">
    <section class="article_text post">

      @RenderBody()

      <section class="article-open_tag">
        @foreach (string tag in Tags.OrderBy(x => x)){
            IDocument tagDocument = Documents[Blog.Tags].FirstOrDefault(x => x.String(BlogKeys.Tag)== tag && x.Get<int>(Keys.CurrentPage) == 1);
            if(tagDocument != null) {
                <a class="tag" href="@Context.GetLink(tagDocument)">@tag</a>
            }
        }
      </section>
    </section>

    <footer class="post">
      <section class="article-open_shere">
        <a class="article-open_shere-twitter" href="https://twitter.com/share?text=Geoffrey%20Huntley%3A%20Introducing%20306%20Million%20Freely%20Downloadable%20Pwned%20Passwords&amp;url=https://www.troyhunt.com/introducing-306-million-freely-downloadable-pwned-passwords/" onclick="window.open(this.href, 'twitter-share', 'width=550,height=235');return false;"><i class="fa fa-twitter"></i> Tweet</a>
        <a class="article-open_shere-facebook" href="https://www.facebook.com/sharer/sharer.php?u=https://www.troyhunt.com/introducing-306-million-freely-downloadable-pwned-passwords/" onclick="window.open(this.href, 'facebook-share','width=580,height=296');return false;"><i class="fa fa-facebook"></i> Post</a>
        <a class="article-open_shere-linkedin" href="https://www.linkedin.com/shareArticle?mini=true&amp;url=https://www.troyhunt.com/introducing-306-million-freely-downloadable-pwned-passwords/" onclick="window.open(this.href, 'facebook-share','width=580,height=296');return false;"><i class="fa fa-linkedin"></i> Update</a>
        <a class="article-open_shere-email" href="https://mail.google.com/mail/?view=cm&amp;fs=1&amp;tf=1&amp;to=&amp;su=Geoffrey%20Huntley%3A%20Introducing%20306%20Million%20Freely%20Downloadable%20Pwned%20Passwords&amp;body=https://www.troyhunt.com/introducing-306-million-freely-downloadable-pwned-passwords/" target="_blank"><i class="fa fa-envelope"></i> Email</a>
        <a class="article-open_shere-feed" href="https://feeds.feedburner.com/TroyHunt"><i class="fa fa-rss"></i> RSS</a>
      </section>

      <section class="article-open_author">
        <div class="article-open_author_bio">
          <h5 itemprop="author" itemscope itemtype="http://schema.org/Person">Geoffrey Huntley</h5>


          <p>@Context.String(BlogKeys.Intro) <a class="article_link-more fa fa-long-arrow-right" href="/about"></a></p>
        </div>
      </section>

      @Html.Partial("_Disqus")

    </footer>
  </article>
</main>
